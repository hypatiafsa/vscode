{
  "name": "Hypatia",
  "patterns": [
    { "include": "#document" }
  ],
  "repository": {
    "document": {
      "patterns": [
        { "include": "#comment"     },
        { "include": "#hypatia"     },
        { "include": "#lingua"      },
        { "include": "#alphabetum"  },
        { "include": "#intervalla"  },
        { "include": "#separatores" },
        { "include": "#lexicon"     },
        { "include": "#mutabilia"   },
        { "include": "#sit"         },
        { "include": "#principium"  },
        { "include": "#productum"   },
        { "include": "#regula"      },
        { "include": "#derivatio"   },
        { "include": "#probatio"    },
        { "include": "#keyword"     },
        { "include": "#symbol"      },
        { "include": "#text"        }
      ]
    },
    "comment": {
      "patterns": [
        {
          "name": "comment.hypatia",
          "match": "--.*"
        },
        {
          "name": "comment.hypatia",
          "begin": "/-",
          "end": "-/"
        }
      ]
    },
    "hypatia": {
      "patterns": [
        {
          "name": "declaration.hypatia.hypatia",
          "begin": "(\\$Hypatia|\\$Hp)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?(?=\\$\\:|$))",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"         },
            "2": { "name": "dialect.hypatia.hypatia" },
            "3": { "name": "text.hypatia"            }
          },
          "end": "(\\$\\:)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "text.hypatia",
              "match": ".+?(?=\\$\\:|$)"
            }
          ]
        }
      ]
    },
    "lingua": {
      "patterns": [
        {
          "name": "declaration.lingua.hypatia",
          "begin": "(\\$Lingua|\\$Ln)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?(?=\\$\\:|$))",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"     },
            "2": { "name": "name.lingua.hypatia" },
            "3": { "name": "text.hypatia"        }
          },
          "end": "(\\$\\:)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "text.hypatia",
              "match": ".+?(?=\\$\\:|$)"
            }
          ]
        }
      ]
    },
    "alphabetum": {
      "patterns": [
        {
          "name": "declaration.alphabetum.hypatia",
          "begin": "(\\$Alphabetum|\\$Al)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.alphabetum.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "intervalla": {
      "patterns": [
        {
          "name": "declaration.intervalla.hypatia",
          "begin": "(\\$Intervalla|\\$In)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.intervalla.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "separatores": {
      "patterns": [
        {
          "name": "declaration.separatores.hypatia",
          "begin": "(\\$Separatores|\\$Sp)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.separatores.hypatia",
              "match": "(?!\\$\\.)(\\S+?(?=\\s|\\$\\.|$))\\s+(?!\\$\\.)(\\S+?(?=\\s|\\$\\.|$))"
            },
            {
              "name": "error.hypatia",
              "match": "(?!\\$\\.)(\\S+?(?=\\s|\\$\\.|$))"
            }
          ]
        }
      ]
    },

    "lexicon": {
      "patterns": [
        {
          "name": "declaration.lexicon.hypatia",
          "begin": "(\\$Lexicon|\\$Lx)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.lexicon.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "mutabilia": {
      "patterns": [
        {
          "name": "declaration.mutabilia.hypatia",
          "begin": "(\\$Mutabilia|\\$Mt)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.mutabilia.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "sit": {
      "patterns": [
        {
          "name": "declaration.sit.hypatia",
          "begin": "(\\$Sit|\\$St)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "error.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },

    "principium": {
      "patterns": [
        {
          "name": "declaration.principium.hypatia",
          "begin": "(\\$Principium|\\$Pr)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?)(\\$\\:)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"        },
            "2": { "name": "name.sententia.hypatia" },
            "3": { "name": "text.hypatia"           },
            "4": { "name": "keyword.hypatia"        }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.sententia.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "productum": {
      "patterns": [
        {
          "name": "declaration.productum.hypatia",
          "begin": "(\\$Productum|\\$Pd)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?)(\\$\\:)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"        },
            "2": { "name": "name.sententia.hypatia" },
            "3": { "name": "text.hypatia"           },
            "4": { "name": "keyword.hypatia"        }
          },
          "end": "(?=\\$Probatio)|(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.sententia.hypatia",
              "match": ".+?(?=\\$Probatio|\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "regula": {
      "patterns": [
        {
          "name": "declaration.regula.hypatia",
          "begin": "(\\$Regula|\\$Rg)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?)(\\$\\:)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"    },
            "2": { "name": "name.ratio.hypatia" },
            "3": { "name": "text.hypatia"       },
            "4": { "name": "keyword.hypatia"    }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.ratio.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "derivatio": {
      "patterns": [
        {
          "name": "declaration.derivatio.hypatia",
          "begin": "(\\$Derivatio|\\$Dr)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?)(\\$\\:)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"    },
            "2": { "name": "name.ratio.hypatia" },
            "3": { "name": "text.hypatia"       },
            "4": { "name": "keyword.hypatia"    }
          },
          "end": "(?=\\$Probatio)|(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.ratio.hypatia",
              "match": ".+?(?=\\$Probatio|\\$\\.|$)"
            }
          ]
        }
      ]
    },

    "probatio": {
      "patterns": [
        {
          "name": "declaration.probatio.hypatia",
          "begin": "(\\$Probatione|\\$Probatio|\\$Pb)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            { "include": "#proof" }
          ]
        }
      ]
    },

    "proof": {
      "patterns": [
        { "include": "#comment"   },
        { "include": "#assumo"    },
        { "include": "#obtineo"   },
        { "include": "#consequor" },
        { "include": "#reitero"   },
        { "include": "#divino"    },
        { "include": "#sit"       },
        { "include": "#assero"    },
        { "include": "#per"       },
        { "include": "#cum"       },
        { "include": "#ex"        },
        { "include": "#probatio"  },
        { "include": "#symbol"    },
        { "include": "#text"      }
      ]
    },

    "assumo": {
      "patterns": [
        {
          "name": "declaration.assumo.hypatia",
          "begin": "(\\$Assumo|\\$As)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$per|\\$pr)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "obtineo": {
      "patterns": [
        {
          "name": "declaration.obtineo.hypatia",
          "begin": "(\\$Obtineo|\\$Ob)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$cum\\$cm)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "consequor": {
      "patterns": [
        {
          "name": "declaration.consequor.hypatia",
          "begin": "(\\$Consequor|\\$Cn)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$ex|\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "reitero": {
      "patterns": [
        {
          "name": "declaration.reitero.hypatia",
          "begin": "(\\$Reitero|\\$Rt)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "divino": {
      "patterns": [
        {
          "name": "declaration.divino.hypatia",
          "begin": "(\\$Divino|\\$Dv)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "assero": {
      "patterns": [
        {
          "name": "declaration.assero.hypatia",
          "begin": "(\\$Assero|\\$Ar)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$ex)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },

    "per": {
      "patterns": [
        {
          "name": "declaration.per.hypatia",
          "match": "(?<=\\$per)\\s*(\\$Principium|\\$Pr|\\$Productum|\\$Pd|\\$Sententiam|\\$Sn)\\s*(\\S*?(?=\\s|\\$))(.*?)(\\$\\.)",
          "captures": {
            "1": { "name": "keyword.hypatia" },
            "2": { "name": "name.hypatia"    },
            "3": { "name": "text.hypatia"    },
            "4": { "name": "keyword.hypatia" }
          }
        },
        {
          "name": "declaration.per.hypatia",
          "match": "(?<=\\$per)\\s*(\\$Regulam|\\$Rg|\\$Derivationem|\\$Dr|\\$Rationem|\\$Rt)\\s*(\\S*?(?=\\s|\\$))(.*?)(\\$cum|\\$cm|\\$\\.)",
          "captures": {
            "1": { "name": "keyword.hypatia" },
            "2": { "name": "name.hypatia"    },
            "3": { "name": "text.hypatia"    },
            "4": { "name": "keyword.hypatia" }
          }
        },
        {
          "name": "declaration.per.hypatia",
          "match": "(?<=\\$per)\\s*(\\$Praemissam|\\$Pm)\\s*(\\$cum|\\$cm|\\$\\.)",
          "captures": {
            "1": { "name": "keyword.hypatia" },
            "2": { "name": "keyword.hypatia" }
          }
        }
      ]
    },
    "cum": {
      "patterns": [
        {
          "name": "declaration.cum.hypatia",
          "begin": "(?<=\\$cum)",
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "error.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "ex": {
      "patterns": [
        {
          "name": "declaration.ex.hypatia",
          "match": "(?<=\\$ex)\\s*(\\$Regula|\\$Rg|\\$Derivatione|\\$Dr|\\$Ratione|\\$Rt)\\s*(\\S*?(?=\\s|\\$))(.*?)(\\$cum|\\$cm|\\$\\.)",
          "captures": {
            "1": { "name": "keyword.hypatia" },
            "2": { "name": "name.hypatia"    },
            "3": { "name": "text.hypatia"    },
            "4": { "name": "keyword.hypatia" }
          }
        }
      ]
    },

    "keyword": {
      "patterns": [
        {
          "name": "error.hypatia",
          "match": "\\$\\.|\\$Vacuum"
        }
      ]
    },
    "symbol": {
      "patterns": [
        {
          "name": "error.hypatia",
          "match": "\\\\nihil|\\\\nh|\\\\spatium|\\\\sp|\\\\tabulatio|\\\\tb|\\\\lineanova|\\\\ln|\\\\caputnovum|\\\\cn"
        }
      ]
    },
    "text": {
      "patterns": [
        {
          "name": "text.hypatia",
          "match": ".+?"
        }
      ]
    }
  },
  "scopeName": "source.hypatia"
}
