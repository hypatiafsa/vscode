{
  "name": "Hypatia",
  "patterns": [
    { "include": "#document" }
  ],
  "repository": {
    "document": {
      "patterns": [
        { "include": "#comments"     },
        { "include": "#hypatia"      },
        { "include": "#lingua"       },
        { "include": "#alphabetum"   },
        { "include": "#intervalla"   },
        { "include": "#separatores"  },
        { "include": "#lexicon"      },
        { "include": "#mutabilia"    },
        { "include": "#sit"          },
        { "include": "#principium"   },
        { "include": "#productum"    },
        { "include": "#regula"       },
        { "include": "#derivatio"    },
        { "include": "#probatio"     },
        { "include": "#keyword"      },
        { "include": "#escapedwords" },
        { "include": "#text"         }
      ]
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.hypatia",
          "match": "--.*"
        },
        {
          "name": "comment.hypatia",
          "begin": "/-",
          "end": "-/"
        }
      ]
    },
    "hypatia": {
      "patterns": [
        {
          "name": "declaration.hypatia.hypatia",
          "begin": "(\\$Hypatia)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?(?=\\$\\:|$))",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"         },
            "2": { "name": "dialect.hypatia.hypatia" },
            "3": { "name": "text.hypatia"            }
          },
          "end": "(\\$\\:)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "text.hypatia",
              "match": ".+?(?=\\$\\:|$)"
            }
          ]
        }
      ]
    },
    "lingua": {
      "patterns": [
        {
          "name": "declaration.lingua.hypatia",
          "begin": "(\\$Lingua)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?(?=\\$\\:|$))",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"     },
            "2": { "name": "name.lingua.hypatia" },
            "3": { "name": "text.hypatia"        }
          },
          "end": "(\\$\\:)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "text.hypatia",
              "match": ".+?(?=\\$\\:|$)"
            }
          ]
        }
      ]
    },
    "alphabetum": {
      "patterns": [
        {
          "name": "declaration.alphabetum.hypatia",
          "begin": "(\\$Alphabetum)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.alphabetum.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "intervalla": {
      "patterns": [
        {
          "name": "declaration.intervalla.hypatia",
          "begin": "(\\$Intervalla)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.intervalla.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "separatores": {
      "patterns": [
        {
          "name": "declaration.separatores.hypatia",
          "begin": "(\\$Separatores)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.separatores.hypatia",
              "match": "(?!\\$\\.)(\\S+?(?=\\s|\\$\\.|$))\\s+(?!\\$\\.)(\\S+?(?=\\s|\\$\\.|$))"
            },
            {
              "name": "error.hypatia",
              "match": "(?!\\$\\.)(\\S+?(?=\\s|\\$\\.|$))"
            }
          ]
        }
      ]
    },

    "lexicon": {
      "patterns": [
        {
          "name": "declaration.lexicon.hypatia",
          "begin": "(\\$Lexicon)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.lexicon.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "mutabilia": {
      "patterns": [
        {
          "name": "declaration.mutabilia.hypatia",
          "begin": "(\\$Mutabilia)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.mutabilia.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "sit": {
      "patterns": [
        {
          "name": "declaration.sit.hypatia",
          "begin": "(\\$Sit)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "error.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },

    "principium": {
      "patterns": [
        {
          "name": "declaration.principium.hypatia",
          "begin": "(\\$Principium)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?)(\\$\\:)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"        },
            "2": { "name": "name.sententia.hypatia" },
            "3": { "name": "text.hypatia"           },
            "4": { "name": "keyword.hypatia"        }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.sententia.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "productum": {
      "patterns": [
        {
          "name": "declaration.productum.hypatia",
          "begin": "(\\$Productum)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?)(\\$\\:)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"        },
            "2": { "name": "name.sententia.hypatia" },
            "3": { "name": "text.hypatia"           },
            "4": { "name": "keyword.hypatia"        }
          },
          "end": "(?=\\$Probatio)|(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.sententia.hypatia",
              "match": ".+?(?=\\$Probatio|\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "regula": {
      "patterns": [
        {
          "name": "declaration.regula.hypatia",
          "begin": "(\\$Regula)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?)(\\$\\:)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"    },
            "2": { "name": "name.ratio.hypatia" },
            "3": { "name": "text.hypatia"       },
            "4": { "name": "keyword.hypatia"    }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.ratio.hypatia",
              "match": ".+?(?=\\$\\.|$)"
            }
          ]
        }
      ]
    },
    "derivatio": {
      "patterns": [
        {
          "name": "declaration.derivatio.hypatia",
          "begin": "(\\$Derivatio)\\s*(\\S*?(?=\\s|\\$\\:|$))(.*?)(\\$\\:)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia"    },
            "2": { "name": "name.ratio.hypatia" },
            "3": { "name": "text.hypatia"       },
            "4": { "name": "keyword.hypatia"    }
          },
          "end": "(?=\\$Probatio)|(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.ratio.hypatia",
              "match": ".+?(?=\\$Probatio|\\$\\.|$)"
            }
          ]
        }
      ]
    },

    "probatio": {
      "patterns": [
        {
          "name": "declaration.probatio.hypatia",
          "begin": "(\\$Probatione|\\$Probatio)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            { "include": "#assumo"    },
            { "include": "#obtineo"   },
            { "include": "#consequor" },
            { "include": "#reitero"   },
            { "include": "#divino"    },
            { "include": "#assero"    },
            { "include": "#sit"       },
            { "include": "#per"       },
            { "include": "#cum"       },
            { "include": "#ex"        },
            { "include": "#probatio"  },
            {
              "name": "text.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },

    "assumo": {
      "patterns": [
        {
          "name": "declaration.assumo.hypatia",
          "begin": "(\\$Assumo)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$per)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "obtineo": {
      "patterns": [
        {
          "name": "declaration.obtineo.hypatia",
          "begin": "(\\$Obtineo)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$cum)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "consequor": {
      "patterns": [
        {
          "name": "declaration.consequor.hypatia",
          "begin": "(\\$Consequor)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$ex|\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "reitero": {
      "patterns": [
        {
          "name": "declaration.reitero.hypatia",
          "begin": "(\\$Reitero)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "divino": {
      "patterns": [
        {
          "name": "declaration.divino.hypatia",
          "begin": "(\\$Divino)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "assero": {
      "patterns": [
        {
          "name": "declaration.assero.hypatia",
          "begin": "(\\$Assero)",
          "beginCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "end": "(\\$ex)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "content.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },

    "per": {
      "patterns": [
        {
          "name": "declaration.per.hypatia",
          "match": "(?<=\\$per)\\s*(\\$Principium|\\$Productum|\\$Sententiam)\\s*(\\S*?(?=\\s|\\$))(.*?)(\\$\\.)",
          "captures": {
            "1": { "name": "keyword.hypatia" },
            "2": { "name": "name.hypatia"    },
            "3": { "name": "text.hypatia"    },
            "4": { "name": "keyword.hypatia" }
          }
        },
        {
          "name": "declaration.per.hypatia",
          "match": "(?<=\\$per)\\s*(\\$Regulam|\\$Derivationem|\\$Rationem)\\s*(\\S*?(?=\\s|\\$))(.*?)(\\$cum|\\$\\.)",
          "captures": {
            "1": { "name": "keyword.hypatia" },
            "2": { "name": "name.hypatia"    },
            "3": { "name": "text.hypatia"    },
            "4": { "name": "keyword.hypatia" }
          }
        },
        {
          "name": "declaration.per.hypatia",
          "match": "(?<=\\$per)\\s*(\\$Praemissam)\\s*(\\$cum|\\$\\.)",
          "captures": {
            "1": { "name": "keyword.hypatia" },
            "2": { "name": "keyword.hypatia" }
          }
        }
      ]
    },
    "cum": {
      "patterns": [
        {
          "name": "declaration.cum.hypatia",
          "begin": "(?<=\\$cum)",
          "end": "(\\$\\.)",
          "endCaptures": {
            "1": { "name": "keyword.hypatia" }
          },
          "patterns": [
            {
              "name": "error.hypatia",
              "match": "\\S+"
            }
          ]
        }
      ]
    },
    "ex": {
      "patterns": [
        {
          "name": "declaration.ex.hypatia",
          "match": "(?<=\\$ex)\\s*(\\$Regula|\\$Derivatione|\\$Ratione)\\s*(\\S*?(?=\\s|\\$))(.*?)(\\$cum|\\$\\.)",
          "captures": {
            "1": { "name": "keyword.hypatia" },
            "2": { "name": "name.hypatia"    },
            "3": { "name": "text.hypatia"    },
            "4": { "name": "keyword.hypatia" }
          }
        }
      ]
    },

    "keyword": {
      "patterns": [
        {
          "name": "error.hypatia",
          "match": "\\$\\.|\\$Vacuum"
        }
      ]
    },
    "escapedwords": {
      "patterns": [
        {
          "name": "error.hypatia",
          "match": "\\\\nihil|\\\\spatium|\\\\tabulatio|\\\\lineanova|\\\\caputnovum"
        }
      ]
    },
    "text": {
      "patterns": [
        {
          "name": "text.hypatia",
          "match": ".+?"
        }
      ]
    }
  },
  "scopeName": "source.hypatia"
}
