{
	"$schema": "vscode://schemas/color-theme",
  "name": "Hypatia",
  "type": "dark",
  "colors": {
    "editor.background":                   "#1F1F1F",
    "editor.foreground":                   "#FAFAFA",
    "editor.selectionHighlightBackground": "#AFFAFF",
    "editor.inactiveSelectionBackground":  "#3A3D41",
    "editorLineNumber.foreground":         "#2b91af",
    "editorLineNumber.activeForeground":   "#2b91af",
    "editorIndentGuide.activeBackground1": "#707070",
    "editorIndentGuide.background1":       "#404040",
    "editorInlayHint.background":          "#3E3E3E",
    "editorInlayHint.foreground":          "#A9A8A7"
  },
  "tokenColors": [

    {
      "scope": "dialect.hypatia",
      "settings": {
        "foreground": "#CA00CA"
      }
    },
    {
      "scope": "lingua.hypatia",
      "settings": {
        "foreground": "#DCDCA0"
      }
    },
    {
      "scope": "alphabetum.hypatia",
      "settings": {
        "foreground": "#55FF00"
      }
    },
    {
      "scope": "intervalla.hypatia",
      "settings": {
        "foreground": "#FF5500"
      }
    },
    {
      "scope": "inclusores.hypatia",
      "settings": {
        "foreground": "#00FFAA"
      }
    },
    {
      "scope": "lexicon.hypatia",
      "settings": {
        "foreground": "#55AF55"
      }
    },
    {
      "scope": "mutabilia.hypatia",
      "settings": {
        "foreground": "#AF55AF"
      }
    },
    {
      "scope": "sententia.hypatia",
      "settings": {
        "foreground": "#0FF0A0"
      }
    },
    {
      "scope": "ratio.hypatia",
      "settings": {
        "foreground": "#0FF0A0"
      }
    },
    {
      "scope": "statement.hypatia",
      "settings": {
        "foreground": "#A0F0A0"
      }
    },

    {
      "scope": "keyword.hypatia",
      "settings": {
        "foreground": "#55ACD5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string.hypatia",
      "settings": {
        "foreground": "#AAAA00"
      }
    },
    {
      "scope": "comment.hypatia",
      "settings": {
        "foreground": "#55AA55",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "blob.hypatia",
      "settings": {
        "foreground": "#AAAAAA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "error.hypatia",
      "settings": {
        "foreground": "#F00F0F",
        "fontStyle": "italic"
      }
    }

  ]
}
